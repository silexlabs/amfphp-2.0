<?xml version="1.0" encoding="UTF-8"?>
<services-config>
	<services>
		<service id="amfphp-flashremoting-service" class="flex.messaging.services.RemotingService" messageTypes="flex.messaging.messages.RemotingMessage">
			<destination id="amfphpExamplesGateway">
				<channels>
					<channel ref="amfphpExamplesGateway"/>
				</channels>
				<properties>
					<source>*</source>
				</properties>
			</destination>
		</service>
	</services>
	<channels>
		<channel-definition id="amfphpExamplesGateway" class="mx.messaging.channels.AMFChannel">
			<endpoint uri="../../../Php/" class="flex.messaging.endpoints.AMFEndpoint"/>
		</channel-definition>		

	</channels>
</services-config>